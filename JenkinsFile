pipeline {
  agent any
    
  stages { 

    stage('Navigation'){
      steps{
        dir('module-3') {
          bat 'npm install'
        }
      }
    } 
            
    stage('Test') {
      steps {
        dir('module-3'){
        bat 'npm run test'
        }
      }
    }
  }
   post {
      always {
        archiveArtifacts '*.html'
        junit allowEmptyResults: true, testResults: '*.xml'
      }
   } 
}