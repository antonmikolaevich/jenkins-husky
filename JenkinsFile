pipeline {
  agent any
  
  stages {
     stage('NavtoFolder'){
      steps {
        bat 'cd module-3'
      }
     }

    stage('Build') {
      steps {
          bat 'npm install'
          bat 'npm i wdio'
      }
    }

    stage('Test') {
      steps {
          bat 'npm test'
      }
    }
  }

      post {
        always {
          junit 'reports/*.xml'
        }
      }
}