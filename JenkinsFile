pipeline {
  agent any
    
  stages { 

    stage('Navigation'){
      steps{
        dir("module-3") 
      }
    }

    stage('Build'){
        steps {
            bat 'npm install'
        }
    }   
            
    stage('Test') {
      steps {
        bat 'npm run test'
      }
    }
  }
   post {
      always {
        junit allowEmptyResults: true, testResults: '*.xml'
      }
   } 
}