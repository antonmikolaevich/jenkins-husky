pipeline {
  agent any
  stages {

     stage('NavtoFolder'){
      steps {
        sh 'cd module-3'
      }
     }

    stage('Build') {
      steps {
          sh 'npm install'
          sh 'npm i wdio'
      }
    }
    stage('Test') {
      steps {
        {
          sh 'npm test'
        }
      }
    }
  }

      post {
        always {
          junit 'reports/*.xml'
        }
      }
    
  
}